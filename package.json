{
  "name": "ankara",
  "version": "0.1.0",
  "description": "Code coverage tool leveraging babylon to cover es6/es7 and strawman proposals",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run typescript && npm run babel",
    "typescript": "tsc --outDir temp",
    "babel": "babel --out-dir dist temp",
    "test": "./tests/run.js |faucet",
    "watch": "npm run typescript-watch & npm run babel-watch",
    "typescript-watch": "npm run typescript -- --watch",
    "babel-watch": "npm run babel -- --watch"
  },
  "author": {
    "name": "Markus Wolf",
    "email": "knister.peter@shadowrun-clan.de"
  },
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "KnisterPeter/ankara"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 2.5.1"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "es6-promisify": "^3.0.0",
    "faucet": "0.0.1",
    "glob": "^5.0.14",
    "tape": "^4.2.0",
    "typescript": "^1.6.2"
  },
  "dependencies": {
    "babylon": "^5.8.23"
  }
}
